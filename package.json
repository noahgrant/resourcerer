    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-sift": "^0.3.0",
    "imports-loader": "^0.8.0",
    "jasmine-core": "^2.99.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "https://registry.npmjs.org/karma-chrome-launcher/-/karma-chrome-launcher-2.2.0.tgz",
    "karma-jasmine": "^1.1.0",
