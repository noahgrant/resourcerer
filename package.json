{
  "name": "resourcerer",
  "version": "1.2.3",
  "repository": "github.com/noahgrant/resourcerer",
  "description": "Declarative data-fetching and caching framework for React",
  "keywords": ["declarative data-fetching framework", "react"],
  "files": ["*.js", "index.d.ts", "lib/*.js", "test/*.js", "*.md", "docs/*.md"],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "scripts": {
    "checks": "npm typecheck && npm test && npm run lint",
    "lint": "eslint lib/*.ts* test/*.js*",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand"
  },
  "jest": {
    "collectCoverageFrom": ["lib/*.{js,jsx}", "!lib/constants.js"],
    "moduleDirectories": ["node_modules"],
    "setupFilesAfterEnv": ["./test/setup"],
    "testEnvironment": "jsdom"
  },
  "exports": "./index.js",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.14.1",
    "webpack": "^4.29.6",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
  }
}
